{
  "env": {
    "commonjs": true,
    "es6": true,
    "node": true
  },
  "extends": ["airbnb-base"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "arrow-parens": "off",
    "comma-dangle": "off",
    "implicit-arrow-linebreak": "off",
    "no-shadow": "off",
    "no-underscore-dangle": "warn",
    "operator-linebreak": "off",
    "consistent-return": "off",
    "function-paren-newline": "off",
    "no-console": "off",
    "import/no-unresolved": "warn"
  }
}
